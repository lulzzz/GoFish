@model GoFish.UI.MVC.Inventory.HomeViewModel

<h2>Your inventory</h2>

<div class="list-group">
	@foreach(var stockItem in Model.Stock) {
        <a asp-controller="Stock"
            asp-action="Summary"
            asp-route-id="@stockItem.ProductType.Id"
            class="list-group-item">
            <h3>@stockItem.ProductType.Name</h3>
        </a>
    }
</div>
<div>
	<a href="@Model.DashboardUrl" class="btn btn-default">Dashboard</a>
</div>